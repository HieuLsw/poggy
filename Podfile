use_frameworks!
def shared_pods
  pod 'Alamofire', '~> 3.3'
end
target 'Poggy' do
  platform :ios, '9.0'
  shared_pods
  pod 'OAuthSwift', '~> 0.5.0'
  pod 'ObjectMapper', '~> 1.0'
  pod 'AlamofireObjectMapper', '~> 2.1'
end
target 'Poggy WatchKit Extension' do
  platform :watchos, '2.0'
  shared_pods
end